{
  "name": "@jan_hoeck/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./typography": {
      "import": {
        "types": "./dist/typography.d.ts",
        "default": "./dist/typography.js"
      },
      "require": {
        "types": "./dist/typography.d.ts",
        "default": "./dist/typography.cjs"
      }
    },
    "./timeline": {
      "import": {
        "types": "./dist/timeline.d.ts",
        "default": "./dist/timeline.js"
      },
      "require": {
        "types": "./dist/timeline.d.ts",
        "default": "./dist/timeline.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@jan_hoeck/eslint-config": "workspace:*",
    "@jan_hoeck/tailwind-config": "workspace:*",
    "@jan_hoeck/typescript-config": "workspace:*",
    "@tailwindcss/vite": "^4.1.8",
    "@turbo/gen": "^2.5.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^3.10.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "postcss": "^8.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.8",
    "typescript": "5.8.2",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.22.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}